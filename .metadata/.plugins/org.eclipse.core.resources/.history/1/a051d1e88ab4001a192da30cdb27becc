
public class conta {

		protected String NomeTitular;
		protected String CPf;
		protected int NumeroConta;
		protected float Saldo;
		
		protected conta(String nomeTitular, String cPf, int numeroConta, float saldo) {
			super();
			NomeTitular = nomeTitular;
			CPf = cPf;
			NumeroConta = numeroConta;
			Saldo = saldo;
		}

		protected String getNomeTitular() {
			return NomeTitular;
		}

		protected void setNomeTitular(String nomeTitular) {
			NomeTitular = nomeTitular;
		}

		protected String getCPf() {
			return CPf;
		}

		protected void setCPf(String cPf) {
			CPf = cPf;
		}

		protected int getNumeroConta() {
			return NumeroConta;
		}

		protected void setNumeroConta(int numeroConta) {
			NumeroConta = numeroConta;
		}

		protected float getSaldo() {
			return Saldo;
		}

		protected void setSaldo(float saldo) {
			Saldo = saldo;
		}
		
	
}
