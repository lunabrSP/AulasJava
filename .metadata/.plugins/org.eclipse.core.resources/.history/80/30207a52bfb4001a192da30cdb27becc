
public class QuestaoComDica extends QuestaoSimples{

	protected String Dica;

	protected QuestaoComDica(String enunciado, String resposta, String Dica) {
		super(enunciado, resposta);
		this.Dica = Dica;
	}

	protected String getDica() {
		return Dica;
	}

	protected void setDica(String dica) {
		Dica = dica;
	}

	@Override
	public String aplicarQuestao(String Resposta, String Dica) {
		// TODO Auto-generated method stub
		return super.aplicarQuestao(Resposta);
	}
	
	
		
}
